import java.awt.*;
import javax.swing.*;

public class Game extends JFrame{
	
	public Game() {
		
		Container c = this.getContentPane();
		c.setLayout(null);
		
	    ImageIcon img  = new ImageIcon("charter.png");
	    JLabel charter  = new JLabel(img);
	    charter.setBounds(500, 300, 200, 700);
	    add(charter);
	    
	    ImageIcon img1  = new ImageIcon("background.jpg");
	    JLabel background  = new JLabel(img1);
	    background.setBounds(0, 0, 1200, 1000);
	    c.add(background);
	    
		setTitle("신과 함께");
		setSize(1200,1000);
		setResizable(false);
		setVisible(true);
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);		
	}

	public static void main(String[] args) {
		new Game();
	}

}
