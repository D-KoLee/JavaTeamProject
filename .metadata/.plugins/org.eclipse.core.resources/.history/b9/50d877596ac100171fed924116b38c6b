import java.awt.*;
import javax.swing.*;

public class Game extends JFrame{
	
	public Game() {
		
		Container c = this.getContentPane();
		c.setLayout(null);
		
	    ImageIcon img  = new ImageIcon("a.png");
	    JLabel charter  = new JLabel(img);
	    charter.setBounds(100, 100, 200, 1000);
	    add(charter);
	    
		setTitle("신과 함께");
		setSize(1200,1000);
		setResizable(false);
		setVisible(true);
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);		
	}

	public static void main(String[] args) {
		new Game();
	}

}
